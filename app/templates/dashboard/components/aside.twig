<aside id="sidebar" class="fixed top-0 left-0 z-20 flex flex-col flex-shrink-0 hidden w-64 h-full pt-16 font-normal duration-75 lg:flex transition-width" aria-label="Sidebar">
	<div class="relative flex flex-col flex-1 min-h-0 pt-0 bg-white border-r border-gray-200 dark:bg-gray-800 dark:border-gray-700">
		<div class="flex flex-col flex-1 pt-5 pb-4 overflow-y-auto">
			<div class="flex-1 px-3 space-y-1 bg-white divide-y divide-gray-200 dark:bg-gray-800 dark:divide-gray-700">
				<ul class="pb-2 space-y-2">
					<li>
						<a href="/" class="flex items-center p-2 text-base text-gray-900 rounded-lg hover:bg-gray-100 group dark:text-gray-200 dark:hover:bg-gray-700">
							<svg class="w-6 h-6 text-gray-500 transition duration-75 group-hover:text-gray-900 dark:text-gray-400 dark:group-hover:text-white" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewbox="0 0 24 24">
								<path d="m19.707 9.293-2-2-7-7a1 1 0 0 0-1.414 0l-7 7-2 2a1 1 0 0 0 1.414 1.414L2 10.414V18a2 2 0 0 0 2 2h3a1 1 0 0 0 1-1v-4a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1v4a1 1 0 0 0 1 1h3a2 2 0 0 0 2-2v-7.586l.293.293a1 1 0 0 0 1.414-1.414Z"/>
							</svg>
							<span class="ml-3" sidebar-toggle-item="">Home</span>
						</a>
					</li>
				</ul>

				<ul class="pb-2 space-y-2">
					{% set menu = "" %}
					{% set seccion = "" %}
					{% set cont = 0 %}
					{% for m in menu_user %}
						{% if menu != m.menu %}
							{% if cont >= 1 %}
							</ul>
						</li>
					{% endif %}
					{% if seccion != m.seccion %}
						<li class="flex items-center p-2 text-base text-gray-900 rounded-lg group dark:text-gray-200">
							{{ m.seccion|raw|convert_encoding('UTF-8', 'ISO-8859-1')|upper }}
						</li>
					{% endif %}


					<li class="{{ url_actual == m.url_menu or url_actual == m.url_submenu ? 'bg-gray-700 text-dark':''}}">

						{% if m.smenu == 0 %}
							<a href="{{ m.url_menu }}" title="{{ m.desc_menu }}" class="{{ 'flex items-center p-2 text-base text-gray-900 rounded-lg hover:bg-gray-100 group dark:text-gray-200 dark:hover:bg-gray-700' }}">

								<i class="text-gray-500 dark:text-gray-400 text-2xl {{" "~ m.ico_menu }}" sidebar-toggle-item></i>
								<span class="ml-3" sidebar-toggle-item>
									{{ m.menu|raw|convert_encoding('UTF-8', 'ISO-8859-1')|upper }}
								</span>
							</a>
						{% else %}
							<button type="button" title="{{ m.desc_menu }}" class="{{ 'flex items-center w-full p-2 text-base text-gray-900 transition duration-75 rounded-lg group hover:bg-gray-100 dark:text-gray-200 dark:hover:bg-gray-700' }}" aria-controls="dropdown-{{ m.id_menu }}" data-collapse-toggle="dropdown-{{ m.id_menu }}">
								<i class="flex-shrink-0 text-gray-500 dark:text-gray-400 text-2xl {{" "~ m.ico_menu }}" aria-hidden="true"></i>
								<span class="flex-1 ml-3 text-left whitespace-nowrap" sidebar-toggle-item>
									{{ m.menu|raw|convert_encoding('UTF-8', 'ISO-8859-1')|upper }}
								</span>
								<i class="bi bi-caret-right-fill text-xs" sidebar-toggle-item></i>
							</button>
						{% endif %}


						<ul id="dropdown-{{ m.id_menu }}" class="hidden py-2 space-y-2">
						{% endif %}
						{% if m.desc_submenu != "" %}
							<li>
								<a href="{{ m.url_submenu }}" title="{{ m.desc_submenu }}" class="flex items-center p-2 text-base text-gray-900 transition duration-75 rounded-lg pl-5 group hover:bg-gray-100 dark:text-gray-200 dark:hover:bg-gray-700 {{ url_actual == m.url_submenu ? 'bg-gray-700 text-dark':''}}">
									<i class="mr-3 text-xs {{ " "~m.ico_submenu }}" aria-hidden="true"></i>
									{{ m.submenu }}
								</a>
							</li>
						{% endif %}
						{% set menu = m.menu %}
						{% set seccion = m.seccion %}
						{% set cont = cont + 1 %}
					{% endfor %}
					{% if cont >= 1 %}
					</ul>
				</li>
			{% endif %}
		</ul>

		<div class="pt-2 space-y-2">
			{% if current_user.role =='admin' %}
				<a href="/admin/usuarios" class="flex items-center p-2 text-base text-gray-900 transition duration-75 rounded-lg hover:bg-gray-100 group dark:text-gray-200 dark:hover:bg-gray-700">
					<svg class="flex-shrink-0 w-6 h-6 text-gray-500 transition duration-75 group-hover:text-gray-900 dark:text-gray-400 dark:group-hover:text-white" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewbox="0 0 24 24">
						<path fill-rule="evenodd" d="M14 2a3.963 3.963 0 0 0-1.4.267 6.439 6.439 0 0 1-1.331 6.638A4 4 0 1 0 14 2Zm1 9h-1.264A6.957 6.957 0 0 1 15 15v2a2.97 2.97 0 0 1-.184 1H19a1 1 0 0 0 1-1v-1a5.006 5.006 0 0 0-5-5ZM6.5 9a4.5 4.5 0 1 0 0-9 4.5 4.5 0 0 0 0 9ZM8 10H5a5.006 5.006 0 0 0-5 5v2a1 1 0 0 0 1 1h11a1 1 0 0 0 1-1v-2a5.006 5.006 0 0 0-5-5Z" clip-rule="evenodd"></path>
					</svg>
					<span class="ml-3" sidebar-toggle-item="">Usuarios</span>
				</a>
				<a href="/admin/perfiles" class="flex items-center p-2 text-base text-gray-900 transition duration-75 rounded-lg hover:bg-gray-100 group dark:text-gray-200 dark:hover:bg-gray-700">
					<svg class="flex-shrink-0 w-6 h-6 text-gray-500 transition duration-75 group-hover:text-gray-900 dark:text-gray-400 dark:group-hover:text-white" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewbox="0 0 24 24">
						<path fill-rule="evenodd" d="M10.8 5a3 3 0 0 0-5.6 0H4a1 1 0 1 0 0 2h1.2a3 3 0 0 0 5.6 0H20a1 1 0 1 0 0-2h-9.2ZM4 11h9.2a3 3 0 0 1 5.6 0H20a1 1 0 1 1 0 2h-1.2a3 3 0 0 1-5.6 0H4a1 1 0 1 1 0-2Zm1.2 6H4a1 1 0 1 0 0 2h1.2a3 3 0 0 0 5.6 0H20a1 1 0 1 0 0-2h-9.2a3 3 0 0 0-5.6 0Z" clip-rule="evenodd"/>
					</svg>
					<span class="ml-3" sidebar-toggle-item="">Perfiles</span>
				</a>
				<a href="/admin/modulesRoutes" class="flex items-center p-2 text-base text-gray-900 transition duration-75 rounded-lg hover:bg-gray-100 group dark:text-gray-200 dark:hover:bg-gray-700">
					<svg class="flex-shrink-0 w-6 h-6 text-gray-500 transition duration-75 group-hover:text-gray-900 dark:text-gray-400 dark:group-hover:text-white" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewbox="0 0 24 24">
						<path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="1" d="M21 13v-2a1 1 0 0 0-1-1h-.757l-.707-1.707.535-.536a1 1 0 0 0 0-1.414l-1.414-1.414a1 1 0 0 0-1.414 0l-.536.535L14 4.757V4a1 1 0 0 0-1-1h-2a1 1 0 0 0-1 1v.757l-1.707.707-.536-.535a1 1 0 0 0-1.414 0L4.929 6.343a1 1 0 0 0 0 1.414l.536.536L4.757 10H4a1 1 0 0 0-1 1v2a1 1 0 0 0 1 1h.757l.707 1.707-.535.536a1 1 0 0 0 0 1.414l1.414 1.414a1 1 0 0 0 1.414 0l.536-.535 1.707.707V20a1 1 0 0 0 1 1h2a1 1 0 0 0 1-1v-.757l1.707-.708.536.536a1 1 0 0 0 1.414 0l1.414-1.414a1 1 0 0 0 0-1.414l-.535-.536.707-1.707H20a1 1 0 0 0 1-1Z"/>
						<path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="1" d="M12 15a3 3 0 1 0 0-6 3 3 0 0 0 0 6Z"/>
					</svg>
					<span class="ml-3" sidebar-toggle-item="">MÃ³dulos</span>
				</a>
			{% endif %}
		</div>
	</div>
</div></div></aside>
